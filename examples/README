# List current DBs
curl -vs localhost:8081/v1/database

# Add DB
curl -vsd @exampleDB.json localhost:8081/v1/database

# View the DB
curl -vs localhost:8081/v1/database/testdb

# Remove the DB
curl -vX DELETE -s localhost:8081/v1/database/testdb

# Add a table to that DB
curl -vsd @exampleDBTable.json localhost:8081/v1/database/testdb

# View the table
curl -vs localhost:8081/v1/database/testdb/table2

# Remove the table
curl -vX DELETE -s localhost:8081/v1/database/testdb/table2
